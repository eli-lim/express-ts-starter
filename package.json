{
  "name": "express-ts-starter",
  "version": "1.0.0",
  "description": "A simple starter for express + typescript + jest",
  "main": "index.js",
  "repository": "https://github.com/elihuansen/express-ts-starter.git",
  "author": "elihuansen",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start:dev": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:prod": "node dist/index.js",
    "build": "tsc",
    "test": "jest src/tests --watch --verbose false --runInBand"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/supertest": "^2.0.10",
    "body-parser": "^1.19.0",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@types/pg": "^7.14.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "pg": "^8.3.3",
    "sql-template-strings": "^2.2.2"
  }
}
